@using Thinktecture.IdentityServer.Web.Areas.Admin.App_LocalResources.Diagnostics
@model Thinktecture.IdentityServer.Models.Configuration.DiagnosticsConfiguration

@{
    ViewBag.Title = Index_cshtml.Diagnostics;
}

@using (Html.BeginForm("index", "Diagnostics", FormMethod.Post, new { @class = "form-horizontal" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(Index_cshtml.ErrorUpdatingDiagnostics, new { @class = "alert alert-error" })
    <fieldset class="editor">
        <legend>@ViewBag.Title</legend>

        @Html.EditorForModel()

        <div class="control-group">
            <div class="controls">
                <button type="submit" class="btn btn-large btn-primary">@Index_cshtml.SaveChanges</button>
                <button type="reset" class="btn btn-large">@Index_cshtml.Cancel</button>
            </div>
        </div>
    </fieldset>
}